{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Example",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/build/example/server.js",
            "stopOnEntry": false,
            "args": ["start"],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "internalConsole",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/build/example/server.js",
                "${workspaceRoot}/build/example/server.js.map",
                "${workspaceRoot}/build/**/*.js",
                "${workspaceRoot}/build/**/*.js.map"
            ]
        }
    ]
}