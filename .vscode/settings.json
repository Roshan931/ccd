{
    "typescript.check.workspaceVersion": true,
    "typescript.tsdk": "./node_modules/typescript/lib",
    "files.exclude": {
        "./src/**/.git": true,
        "./src/**/.svn": true,
        "./src/**/.hg": true,
        "./src/**/.DS_Store": true,
        "./src/*.js":{"when": "$(basename).ts"},
        "./src/*.js.map":true
    }
}